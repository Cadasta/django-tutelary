{
  "version": "2015-12-10",
  "clause": [
    { "effect": "allow",
      "action": ["party.list"],
      "object": ["project/$organisation/*"] },
    { "effect": "allow",
      "action": ["party.view"],
      "object": ["party/$organisation/*/*"] },
    { "effect": "allow",
      "action": ["parcel.list"],
      "object": ["project/$organisation/*"] },
    { "effect": "allow",
      "action": ["parcel.view"],
      "object": ["parcel/$organisation/*/*"] }
  ]
}
